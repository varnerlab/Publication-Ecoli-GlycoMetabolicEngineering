	Keywords = {constraint-based modeling; FBA; heuristic optimization; OptGene; OptKnock; metabolic engineering; metabolism;},
	Mesh = {Algorithms; Benzaldehydes; Computational Biology; Computer Simulation; Escherichia coli; Evolution, Molecular; Gene Expression Regulation; Genetic Engineering; Genome, Fungal; Glycerol; Models, Biological; Models, Genetic; Phenotype; Protein Engineering; Protein Interaction Mapping; Saccharomyces cerevisiae; Software; Succinic Acid},
