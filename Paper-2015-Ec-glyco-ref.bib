	Journal = {Biochemistry},
	Journal-Full = {Biochemistry},
	Keywords = {glycosylation; C. jejuni; pglC;},
	Mesh = {Campylobacter jejuni; Carbohydrate Conformation; Catalysis; Cloning, Molecular; Detergents; Gene Expression; Glycosylation; Phosphotransferases; Polysaccharides; Uridine Diphosphate Sugars},
