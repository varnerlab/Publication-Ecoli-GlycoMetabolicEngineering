	Pages = {295-301},
	Pmid = {12670695},
	Pst = {ppublish},
	Title = {Analysis of metabolic and physiological responses to gnd knockout in Escherichia coli by using C-13 tracer experiment and enzyme activity measurement},
	Volume = {220},
