	Keywords = {glycosylation; C. jejuni; pglD;},
	Mesh = {Acetylglucosamine; Acetyltransferases; Campylobacter jejuni; Cloning, Molecular; Kinetics; Metabolic Networks and Pathways; Nuclear Magnetic Resonance, Biomolecular; Polysaccharides; Spectrometry, Mass, Electrospray Ionization},
