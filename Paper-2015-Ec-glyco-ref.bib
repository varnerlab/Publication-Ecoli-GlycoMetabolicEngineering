	Keywords = {glycosylation; E. coli; YebF; C. jejuni; protein production;},
	Mesh = {Bacterial Proteins; Campylobacter jejuni; Culture Media; Escherichia coli; Escherichia coli Proteins; Glycoproteins; Glycosylation; Periplasm; Polysaccharides; Protein Engineering; Protein Transport; Recombinant Proteins; Signal Recognition Particle},
