	Keywords = {glycosylation; C. jejuni; E. coli; pglB;},
	Mesh = {Bacterial Vaccines; Escherichia coli; Escherichia coli Proteins; Glycosylation; Hexosyltransferases; Lipopolysaccharides; Lipoproteins; Membrane Proteins; Membrane Transport Proteins; O Antigens; Protein Engineering; Vaccines, Conjugate},
	Month = {Feb},
